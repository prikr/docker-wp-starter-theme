{
  "name": "prikr-wp-starter",
  "description": "official prikr wordpress theme",
  "author": {
    "name": "Jasper van Doorn",
    "email": "jasper@prikr.io",
    "url": "https://prikr.io"
  },
  "keywords": [
    "prikr-wp-starter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/prikr/prikr-wp-starter"
  },
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "predev": "set cross-env NODE_ENV=development",
    "dev": "gulp --require @babel/register",
    "style": "cross-env NODE_ENV=development gulp style --require @babel/register",
    "fonts": "cross-env NODE_ENV=development gulp fonts --require @babel/register",
    "prebuild": "set cross-env NODE_ENV=production",
    "build": "cross-env NODE_ENV=production gulp build --require @babel/register",
    "push:develop": "git push origin develop && git ftp push -s develop",
    "push:master": "git push origin master && git ftp push -s productie"
  },
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "license": "MIT",
  "dependencies": {
    "@animated-burgers/burger-squeeze": "^1.1.2",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "autoprefixer": "^10.2.1",
    "bootstrap": "^5.0.0-beta1",
    "crumbsjs": "^0.2.1",
    "fs": "0.0.1-security",
    "gulp-google-webfonts": "^4.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "isotope-layout": "^3.0.6",
    "popper.js": "^1.16.0",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-loader": "^4.1.0",
    "swiper": "^6.4.10",
    "vanilla-lazyload": "^17.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "@popperjs/core": "^2.6.0",
    "aos": "^2.3.4",
    "browser-sync": "^2.26.13",
    "cross-env": "^7.0.3",
    "gulp": "^4.0.2",
    "gulp-cache": "^1.1.3",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^4.0.1",
    "gulp-debug": "^4.0.0",
    "gulp-header-comment": "^0.9.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-postcss": "^9.0.0",
    "gulp-purgecss": "^3.1.3",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-terser": "^1.4.1",
    "gulp-uglify-es": "^2.0.0",
    "path": "^0.12.7",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.15.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0"
  }
